<%- include("../partials/header") %>


<div class="form-container">
    <h1>Register</h1>

    <form action="/auth/register" method="POST">
        <label for="username">username</label>
        <input id="username" type="text" name="username" placeholder="saia">
        
        <label for="password">password</label>
        <input id="password" type="password" name="password" placeholder="pass123">
    
        <button class="btn btn-primary">submit</button>
    </form>

    <p>want to use google instead? <a class="register-link"   href="/auth/google">google login</a></p>
</div>



<%- include("../partials/footer") %>